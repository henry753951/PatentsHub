datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
  output          = "../../../node_modules/.prisma/client"
}

generator zod {
  provider       = "zod-prisma-types"
  output = "../zod"
}