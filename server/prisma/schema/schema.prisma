generator client {
  provider        = "prisma-client-js"
  output          = "../../../node_modules/@prisma/client/node_modules/.prisma/client"
  previewFeatures = ["prismaSchemaFolder"]
}

generator zod {
  provider = "zod-prisma-types"
  output   = "../zod"
  writeNullishInModelTypes = true
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
